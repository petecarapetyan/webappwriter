<div class="mobile-name">
  <a class="mobile-logo" href="{{ '/' | url }}">
    <img src="{{ '/_assets/logo.svg' | asset | url }}" alt="{{ site.logoAlt }}" />
    <span>{{ site.name }}</span>
  </a>
</div>
<div class="primary-header">
  {% for blockName, blockPath in _joiningBlocks.header %}
    {% include blockPath %}
  {% endfor %}
</div>